{
    "name": "video-tagging",
    "version": "1.0.6",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "electron": "electron .",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pack": "ng build  --prod --base-href ./ && electron-builder",
        "electron-build": "ng build  --prod --base-href ./ && electron .",
        "electron-package": "electron-packager . FarmhubCompanyDashboard --platform=win32 --arch=x64",
        "g": "ng g c shared/region-info"
    },
    "build": {
        "appId": "com.mien.mien",
        "productName": "VideoTagging",
        "files": [
            "**/*",
            "dist/**/*"
        ],
        "directories": {
            "output": "release",
            "buildResources": "dist"
        },
        "asar": false,
        "win": {
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "runAfterFinish": true
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.0.6",
        "@angular/cdk": "~9.2.1",
        "@angular/common": "~9.0.6",
        "@angular/compiler": "~9.0.6",
        "@angular/core": "~9.0.6",
        "@angular/forms": "~9.0.6",
        "@angular/material": "~9.2.1",
        "@angular/platform-browser": "~9.0.6",
        "@angular/platform-browser-dynamic": "~9.0.6",
        "@angular/router": "~9.0.6",
        "@types/snapsvg": "^0.5.0",
        "image-size": "^1.0.0",
        "rxjs": "~6.5.4",
        "shortid": "^2.2.15",
        "tslib": "^1.10.0",
        "video.js": "^7.7.5",
        "vott-ct": "file:src/assets/lib/vott-ct-2.2.16.tgz",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.6",
        "@angular/cli": "~9.0.6",
        "@angular/compiler-cli": "~9.0.6",
        "@angular/language-service": "~9.0.6",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "electron": "^8.2.4",
        "electron-builder": "^22.8.1",
        "electron-packager": "^15.1.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    }
}
